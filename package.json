    "@mui/x-date-pickers": "^6.5.0",
    "date-fns": "^2.30.0",
    "sqlite3": "^5.1.6",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
